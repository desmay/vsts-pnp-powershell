{
  "name": "pnppowershell",
  "version": "0.0.1",
  "description": "PnP-Powershell VSTS task",
  "main": "pnp-powershell.js",
  "scripts": {
    "build": "tsc -p .",
    "package": "npm run build && gulp version && cd ../.. && tfx extension create --manifest-globs vss-extension.json",
    "publish": "npm run package && cd ../.. && tfx extension publish --share-with mvapps-vsts --rev-version"
  },
  "author": "Sergei Sergeev",
  "license": "MIT",
  "dependencies": {
    "uuid": "^3.2.1",
    "vsts-task-lib": "^2.4.0"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.5",
    "@types/node": "^10.3.1",
    "gulp": "^3.9.1",
    "gulp-json-editor": "^2.4.1",
    "gulpclass": "^0.1.2",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  }
}
